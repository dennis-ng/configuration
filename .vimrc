syntax on
set tabstop=2
set shiftwidth=2
set expandtab
set ai
set hlsearch
set ruler
set laststatus=2
if (has("termguicolors"))
  set termguicolors
endif
colorscheme deus
highlight Normal guibg=NONE ctermbg=NONE
highlight NonText guibg=NONE ctermbg=NONE
highlight Search cterm=NONE ctermfg=black ctermbg=green
